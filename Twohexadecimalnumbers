DATA SEGMENT
NUM1  DW 9250H 
NUM2  DW 8040H 
RESULT  DW 2 DUP(0)
DATA ENDS 
CODE  SEGMENT
ASSUME CS:CODE,DS:DATA 
START: MOV AX,DATA; 
       MOV DS,AX;
       MOV AX,NUM1;
       ADD AX,NUM2;
       MOV RESULT ,AX;
       JNC LAST ; 
       MOV RESULT +2,1;
LAST: MOV AH,4CH;
      INT 21H ; 
CODE ENDS
END START 
